<Application x:Class="Client.Administration.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Client.Administration"
             xmlns:viewModels="clr-namespace:Client.Administration.MVVM.ViewModels"
             xmlns:views="clr-namespace:Client.Administration.MVVM.Views"
>
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/WpfShared;component/Resources/Fonts.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WpfShared;component/Resources/Images.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WpfShared;component/Resources/Buttons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WpfShared;component/Resources/ColorPalette.xaml" />
                <ResourceDictionary Source="Resources/Menu.xaml"/>
                <ResourceDictionary Source="Resources/Borders.xaml"/>
                <ResourceDictionary Source="Resources/Buttons.xaml"/>
                <ResourceDictionary Source="Resources/TileDictionary.xaml"/>
                <ResourceDictionary Source="Resources/FontStylingDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
