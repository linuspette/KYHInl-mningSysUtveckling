<Application x:Class="LpSmartHub.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LpSmartHub">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/FontsDictionary.xaml"/>
                <ResourceDictionary Source="Resources/MenuDictionary.xaml"/>
                <ResourceDictionary Source="Resources/BorderDictionary.xaml"/>
                <ResourceDictionary Source="Resources/ButtonDictionary.xaml"/>
                <ResourceDictionary Source="Resources/TileDictionary.xaml"/>
                <ResourceDictionary Source="Resources/FontStylingDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
